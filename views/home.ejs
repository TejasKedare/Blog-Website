<%- include('partials/header'); -%>

<div class="container">
  <h1>Home</h1>
  <p><%= homeStartingContent %></p>
  <% posts.forEach(post => { %>
  <h1><%= post.title %></h1>
  <p>
    <%= post.info.substring(0,100) + "..." %>
    <a href="/posts/<%= post.title %> ">Read More</a>
  </p>

  <% }) %>
</div>
<%- include('partials/footer'); -%>
